{
  "id": "Cube",
  "groups" : {
    "position": [ "group1", "group2" ],
    "group1": {
      "display" : "General",
      "position" : ["name"],
      "fields": {
        "name": {
          "widget": "textbox",
          "label" : "Name"
        }
      }
    },

    "group2" : {
      "display" : "Resolution & Aggregations",
      "position" : [ "dataset.cube.resolutions", "mapping.config", "custom.properties" ],
      "fields" : {
         "dataset.cube.resolutions" : {
           "widget" : "csv",
           "label" : "Cube Resolution",
           "properties": {
              "delimiter": ","
           }
         },
         "mapping.config" : {
            "widget" : "json-editor",
            "label" : "Cube Configuration"
         },
         "custom.properties" : {
           "widget" : "json-editor",
           "label"  : "Custom Dataset Properties"
         }
      }
    }
  }
}
